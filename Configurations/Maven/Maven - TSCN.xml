<?xml version="1.0"?>
<settings>
    <mirrors>
        <mirror>
            <id>public</id>
            <name>Tradeshift Nexus (Releases)</name>
            <url>https://cn.nexus.bwtsi.cn/content/groups/tradeshift-public</url>
            <mirrorOf>central,!snapshots</mirrorOf>
        </mirror>
        <mirror>
            <id>snapshots</id>
            <name>Tradeshift Nexus (Snapshots)</name>
            <url>https://cn.nexus.bwtsi.cn/content/groups/tradeshift-public-snapshots</url>
            <mirrorOf>snapshots</mirrorOf>
        </mirror>
    </mirrors>
    <servers>
        <server>
            <username>{user}</username>
            <password>{password}</password>
            <filePermissions>664</filePermissions>
            <directoryPermissions>775</directoryPermissions>
            <id>tradeshift-plugins</id>
        </server>
        <server>
            <username>{user}</username>
            <password>{password}</password>
            <filePermissions>664</filePermissions>
            <directoryPermissions>775</directoryPermissions>
            <id>tradeshift-public</id>
        </server>
        <server>
            <username>{user}</username>
            <password>{password}</password>
            <filePermissions>664</filePermissions>
            <directoryPermissions>775</directoryPermissions>
            <id>tradeshift-snapshots</id>
        </server>
        <server>
            <username>{user}</username>
            <password>{password}</password>
            <filePermissions>664</filePermissions>
            <directoryPermissions>775</directoryPermissions>
            <id>snapshots</id>
        </server>
        <server>
            <username>{user}</username>
            <password>{password}</password>
            <filePermissions>777</filePermissions>
            <directoryPermissions>777</directoryPermissions>
            <id>releases</id>
        </server>
        <server>
            <username>{user}</username>
            <password>{password}</password>
            <filePermissions>664</filePermissions>
            <directoryPermissions>775</directoryPermissions>
            <id>public</id>
        </server>

    </servers>
    <activeProfiles>
        <activeProfile>tradeshift</activeProfile>
    </activeProfiles>
    <profiles>
        <profile>
            <id>tradeshift</id>
            <activation>
                <activeByDefault>false</activeByDefault>
            </activation>
            <repositories>
                <repository>
                    <releases>
                        <enabled>true</enabled>
                        <updatePolicy>always</updatePolicy>
                        <checksumPolicy>warn</checksumPolicy>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                    <id>tradeshift-public</id>
                    <name>Tradeshift - Releases</name>
                    <url>https://cn.nexus.bwtsi.cn/content/repositories/tradeshift-public</url>
                    <layout>default</layout>
                </repository>
                <repository>
                    <releases>
                        <enabled>false</enabled>
                    </releases>
                    <snapshots>
                        <enabled>true</enabled>
                        <updatePolicy>always</updatePolicy>
                    </snapshots>
                    <id>tradeshift-snapshots</id>
                    <name>Tradeshift - Snapshots</name>
                    <url>https://cn.nexus.bwtsi.cn/content/repositories/tradeshift-public-snapshots</url>
                    <layout>default</layout>
                </repository>
            </repositories>
            <pluginRepositories>
                <pluginRepository>
                    <id>public</id>
                    <url>https://cn.nexus.bwtsi.cn/content/groups/tradeshift-public</url>
                    <releases>
                        <enabled>true</enabled>
                    </releases>
                    <snapshots>
                        <enabled>false</enabled>
                    </snapshots>
                </pluginRepository>
                <pluginRepository>
                    <id>tradeshift-plugins</id>
                    <name>Tradeshift - Plugins</name>
                    <url>https://cn.nexus.bwtsi.cn/content/repositories/tradeshift-public-plugins</url>
                    <layout>default</layout>
                </pluginRepository>
            </pluginRepositories>
        </profile>
    </profiles>
</settings>
